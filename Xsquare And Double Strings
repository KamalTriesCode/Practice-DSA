#include<bits/stdc++.h>
using namespace std;

int main()
{
	int t;
	cin>>t;

	while(t--)
	{


	map<char,int> m;
	string g;
	cin>>g;
	for(auto &a:m)
	{
		a.second=0;
	}
	for(int i=0;i<g.size();i++)
	{
		m[g[i]]++;
	}

	// for(int i=0;i<g.size();i++)
	// {
	// 	cout<<m[i]<<" ";
	// }

	string ans="No";

	for(auto &a:m)
	{
		// cout<<a.second<<" ";

		if(a.second>=2)
		{
			ans="Yes";
			break;
		}
	}

	cout<<ans<<endl;
}
}
